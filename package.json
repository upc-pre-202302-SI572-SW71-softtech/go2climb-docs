{
  "name": "markdown-docs",
  "version": "0.0.0",
  "description": "Write documentation using Markdown, preview as HTML with live reload and render as PDF. Version control your document with Git.",
  "author": {
    "name": "dalbitresb12",
    "email": "me@dalbitresb.com"
  },
  "homepage": "https://github.com/dalbitresb12/markdown-docs",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dalbitresb12/markdown-docs.git"
  },
  "bugs": {
    "url": "https://github.com/dalbitresb12/markdown-docs/issues"
  },
  "license": "MIT",
  "private": true,
  "type": "module",
  "packageManager": "yarn@3.6.3",
  "scripts": {
    "render": "tsx src/index.ts render",
    "watch": "tsx src/index.ts -w render",
    "watch:ts": "tsx watch src/index.ts -w render",
    "serve": "tsx src/index.ts serve",
    "types": "tsc --noemit",
    "lint": "concurrently -s -n lint: \"yarn:lint:*(!fix)\"",
    "lint:ts": "concurrently -s -n lint:ts: \"yarn:lints:ts:*\"",
    "lints:ts:eslint": "eslint --cache \"**/*.ts\"",
    "lints:ts:prettier": "prettier -c \"**/*.ts\"",
    "lint:css": "stylelint --cache \"**/*.css\"",
    "lint:html": "prettier -c \"**/*.(html|handlebars)\"",
    "lint:json": "prettier -c \"**/*.json(c)?\"",
    "lint:md": "concurrently -s -n lint:md: \"yarn:lints:md:*\"",
    "lints:md:cli": "markdownlint-cli2 \"**/*.md\"",
    "lints:md:prettier": "prettier -c \"**/*.md\"",
    "lint:yml": "prettier -c \"**/*.yml\"",
    "lint:fix": "concurrently -s -n lint:fix: \"yarn:lint:fix:*\"",
    "lint:fix:ts": "concurrently -s -n lint:fix:ts: \"yarn:lints:fix:ts:*\"",
    "lints:fix:ts:eslint": "eslint --fix --cache \"**/*.ts\"",
    "lints:fix:ts:prettier": "prettier -w \"**/*.ts\"",
    "lint:fix:css": "stylelint --fix --cache \"**/*.css\"",
    "lint:fix:html": "prettier -w \"**/*.(html|handlebars)\"",
    "lint:fix:json": "prettier -w \"**/*.json(c)?\"",
    "lint:fix:md": "concurrently -s -n lint:fix:md: \"yarn:lints:fix:md:*\"",
    "lints:fix:md:cli": "markdownlint-cli2 --fix \"**/*.md\"",
    "lints:fix:md:prettier": "prettier -w \"**/*.md\"",
    "lint:fix:yml": "prettier -w \"**/*.yml\""
  },
  "dependencies": {
    "@sindresorhus/slugify": "^2.2.1",
    "concurrently": "^8.2.1",
    "consola": "^3.2.3",
    "gray-matter": "^4.0.3",
    "handlebars": "^4.7.8",
    "highlight.js": "^11.8.0",
    "markdown-it": "^13.0.1",
    "markdown-it-anchor": "^8.6.7",
    "markdown-it-attrs": "^4.1.6",
    "markdown-it-highlightjs": "^4.0.1",
    "markdown-it-multimd-table": "^4.2.3",
    "markdown-it-replace-link": "^1.2.0",
    "markdown-it-toc-done-right": "^4.2.0",
    "markedpp": "^1.2.0",
    "tsx": "^3.12.8",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.1.0",
    "@types/eslint": "^8.44.2",
    "@types/markdown-it": "^13.0.1",
    "@types/markdown-it-attrs": "^4.1.0",
    "@types/node": "18",
    "@types/prettier": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-unicorn": "^48.0.1",
    "markdownlint": "^0.30.0",
    "markdownlint-cli2": "^0.9.2",
    "prettier": "^3.0.3",
    "stylelint": "^15.10.3",
    "stylelint-config-standard": "^34.0.0",
    "typescript": "^5.2.2"
  }
}
